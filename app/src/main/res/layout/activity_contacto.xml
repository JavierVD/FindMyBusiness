<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#4E4157"
    tools:context=".contactoActivity">

    <TextView
        android:id="@+id/textView8"
        android:layout_width="216dp"
        android:layout_height="0dp"
        android:layout_marginEnd="88dp"
        android:layout_marginBottom="57dp"
        android:text="Find My Business"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/textView3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/contactanos"
        android:textColor="#FFFFFF"
        android:textSize="36sp"
        app:layout_constraintBottom_toTopOf="@+id/textView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.562" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="346dp"
        android:layout_height="69dp"
        android:layout_marginTop="135dp"
        android:layout_marginBottom="434dp"
        android:text="@string/haz_click_en_el_icono_deseado_para_contactarnos"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/textView5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.557"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.259" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="348dp"
        android:layout_height="0dp"
        android:layout_marginTop="36dp"
        android:text="@string/o_comunicate_a_traves_de_los_siguientes_numeros"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.444"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="93dp"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="116dp"
        android:text="4494596585"
        android:textColor="#FFFDFD"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/btnum1"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="102dp"
        android:layout_height="0dp"
        android:layout_marginTop="400dp"
        android:layout_marginEnd="48dp"
        android:text="4492158116"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <ImageView
        android:id="@+id/btnGoogle"
        android:layout_width="57dp"
        android:layout_height="49dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="34dp"
        android:layout_marginEnd="25dp"
        android:layout_marginBottom="33dp"
        app:layout_constraintBottom_toTopOf="@+id/btnInsta"
        app:layout_constraintEnd_toStartOf="@+id/textView6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/gmail" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="242dp"
        android:layout_height="0dp"
        android:layout_marginTop="217dp"
        android:layout_marginEnd="64dp"
        android:layout_marginBottom="136dp"
        android:text="FindMyBusiness@gmail.com"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/textView8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/btnInsta"
        android:layout_width="71dp"
        android:layout_height="61dp"
        android:layout_marginTop="308dp"
        app:layout_constraintEnd_toStartOf="@+id/textView7"
        app:layout_constraintHorizontal_bias="0.607"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/instagram" />

    <ImageView
        android:id="@+id/btnFace"
        android:layout_width="61dp"
        android:layout_height="51dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="388dp"
        android:layout_marginBottom="148dp"
        app:layout_constraintBottom_toTopOf="@+id/btnum1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/facebook"
        app:tint="#1F6CCC" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="241dp"
        android:layout_height="48dp"
        android:layout_marginTop="48dp"
        android:layout_marginEnd="64dp"
        android:text="FindMyBuss_13"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <ImageView
        android:id="@+id/btnum1"
        android:layout_width="49dp"
        android:layout_height="57dp"
        android:layout_marginStart="32dp"
        android:layout_marginBottom="184dp"
        android:tint="#07AFFB"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/tel" />

    <ImageView
        android:id="@+id/btnnum2"
        android:layout_width="66dp"
        android:layout_height="59dp"
        android:layout_marginTop="195dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="194dp"
        android:tint="#07AFFB"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/textView5"
        app:layout_constraintTop_toTopOf="@+id/textView8"
        app:srcCompat="@drawable/tel" />

</androidx.constraintlayout.widget.ConstraintLayout>